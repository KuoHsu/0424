    "counter = 0\n",
    "while(True):\n",
    "    x = input(\"請輸入任意數字或q：\")\n",
    "    counter+=1\n",
    "    if x == '8':\n",
    "        print(\"猜到了 猜了{0}次\".format(counter))\n",
    "        break\n",
    "    elif x == 'q':\n",
    "        print(\"Bye\")\n",
    "        break\n",
    "    else:\n",
    "        print(\"QQ 沒猜到\")"
